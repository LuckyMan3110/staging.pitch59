{
  "name": "pitch59",
  "version": "0.0.1",
  "license": "",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-host": "ng serve --host 0.0.0.0",
    "start-disable-host": "ng serve --disable-host-check",
    "build:prod": "ng build --configuration=production",
    "build:staging": "ng build --configuration=staging",
    "build:dev": "ng build --configuration=dev",
    "serve": "node server.ts",
    "test": "ng test",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,html,css,scss}\"",
    "format-check": "prettier --check \"./src/**/*.{js,jsx,ts,tsx,html,css,scss}\"",
    "lint": "eslint \"./src/**/*.{ts,tsx,html}\" --quiet --fix",
    "e2e": "ng e2e",
    "scsslint": "sass-lint -v",
    "tsformat": "tsfmt -r",
    "start:fr": "ng serve --aot --i18nFile=src/locale/messages.fr.xlf --i18nFormat=xlf --locale=fr",
    "build:fr": "ng build --configuration production --i18nFile=src/locale/messages.fr.xlf --i18nFormat=xlf --locale=fr",
    "extract": "ng xi18n --outputPath=src/locale",
    "serve:ssr": "node dist/main.js",
    "build:client-and-server-bundles-prod": "ng run pitch59:server -c production && ng build --configuration=production",
    "build:client-and-server-bundles-staging": "ng run pitch59:server -c staging && ng build --configuration=staging",
    "build:client-and-server-bundles-dev": "ng run pitch59:server -c dev && ng build --configuration=dev",
    "build:client-and-server-bundles-demo": "ng run pitch59:server -c demo && ng build --configuration=demo",
    "scully": "npx scully --",
    "scully:serve": "npx scully serve --",
    "prepare": "husky install"
  },
  "private": true,
